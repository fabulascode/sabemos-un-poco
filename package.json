{
  "name": "sabemos-jr",
  "version": "1.0.0",
  "description": "immigration bond data in a graphql server",
  "main": "main.js",
  "author": "Michael Edelman <michael@fabulas.io>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "test": "cross-env NODE_ENV=testing jest --forceExit --detectOpenHandles --silent",
    "dev": "nodemon --exec yarn migrate",
    "migrate": "npx prisma deploy && npx prisma generate && npx prisma reset --force && babel-node scripts/init.js",
    "restart": "rimraf dist && yarn build && yarn start",
    "start": "node dist/index.js"
  },
  "peerDependencies": {
    "babel-core": "6.x || ^7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "winston": "^3.2.1"
  },
  "dependencies": {
    "apollo-link": "^1.2.11",
    "apollo-link-http": "^1.5.14",
    "csvtojson": "^2.0.8",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.5.0",
    "nodemon": "^1.19.0",
    "prisma-client-lib": "^1.30.4"
  }
